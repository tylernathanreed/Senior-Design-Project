            <kind>1</kind>
            <string>//* JS API Trigger *//
// Make sure the API Map Exists
if(!ds_exists(js_api, ds_type_map))
{
    echo("obj_player (" + string(id) + "): Invalid call to JS API! DS Map " + string(js_api) + " does not exist!");
    exit;
}

// Determine the API Attributes
var type, name, value;

type = ds_map_find_value(js_api, "type");
name = ds_map_find_value(js_api, "name");
value = ds_map_find_value(js_api, "value");

switch(type)
{
    case "get":
        // Create the Variable Map
        var variables = ds_map_create();

        // Check for Special Case
        if(name == "all")
        {
            // Register all Variables
            ds_map_add(js_callback, "phys_radius", phys_radius);
            ds_map_add(js_callback, "phys_density", phys_density);
            ds_map_add(js_callback, "phys_friction", phys_friction);
            ds_map_add(js_callback, "phys_restitution", phys_restitution);
            ds_map_add(js_callback, "phys_angular_damping", phys_angular_damping);
            ds_map_add(js_callback, "phys_linear_damping", phys_linear_damping);
            ds_map_add(js_callback, "move_speed", move_speed);
        }
        else
        {
            switch(name)
            {
                case "phys_radius": ds_map_add(js_callback, name, phys_radius); break;
                case "phys_density": ds_map_add(js_callback, name, phys_density); break;
                case "phys_friction": ds_map_add(js_callback, name, phys_friction); break;
                case "phys_restitution": ds_map_add(js_callback, name, phys_restitution); break;
                case "phys_angular_damping": ds_map_add(js_callback, name, phys_angular_damping); break;
                case "phys_linear_damping": ds_map_add(js_callback, name, phys_linear_damping); break;
                case "move_speed": ds_map_add(js_callback, name, move_speed); break;
            }
        }
        break;

    case "set":
        switch(name)
        {
            case "move_speed": move_speed = value; break;
        }
}

// Check for End of Inheritance
if(object_index == obj_player)
{
    // Give the js_callback Map to Java Script
}
